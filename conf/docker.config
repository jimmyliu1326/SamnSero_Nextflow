process {
    withName:porechop {
        container = 'biocontainers/porechop'
    }
    withName:flye {
        container = 'staphb/flye'
    }
    withName:dragonflye {
        container = 'jimmyliu1326/dragonflye:1.0.12'
    }
    withName:sistr {
        container = 'staphb/sistr'
    }
    withName:abricate {
        container = 'staphb/abricate'
    }
    withName:makeblastdb {
        container = 'staphb/abricate'
    }
    withName:abricate_custom {
        container = 'staphb/abricate'
    }
    withName:abricate_summary {
        container = 'staphb/abricate'
    }
    withName:checkm {
        container = 'sstevens/checkm'
    }
    withName:quast {
        container = 'staphb/quast:5.0.2'
    }
    withName:qc_report {
        container = 'jimmyliu1326/samnsero-r:0.5'
    }
    withName:annot_report {
        container = 'jimmyliu1326/samnsero-r:0.5'
    }
    withName:combine_res {
        container = 'jimmyliu1326/samnsero-r:0.5'
    }
    withName:medaka_gpu {
        container = 'ontresearch/medaka:v1.6.0'
        containerOptions = '--gpus 0'
    }
    withName:medaka {
        container = 'nanozoo/medaka:1.6.0--9469527'
    }
    withName:centrifuge {
        container = 'nanozoo/centrifuge'
    }
    withName:krona {
        container = 'nanozoo/krona:2.7.1--e7615f7'
    }
    withName:mob_suite {
        container = 'quay.io/biocontainers/mob_suite:3.0.3--pyhdfd78af_0'
    }
    withName:mob_suite_merge {
        container = 'jimmyliu1326/samnsero-r:0.5'
    }
    withName:rgi {
        container = 'quay.io/biocontainers/rgi:5.2.1--pyha8f3691_2'
    }
    withName:rgi_summary {
        container = 'jimmyliu1326/samnsero-r:0.5'
    }
    withName:mob_suite_summary {
        container = 'jimmyliu1326/samnsero-r:0.5'
    }
    withName:aggregate_rgi {
        container = 'jimmyliu1326/samnsero-r:0.2'
    }
    withName:aggregate_mob_suite {
        container = 'jimmyliu1326/samnsero-r:0.2'
    }
    withName:aggregate_sistr {
        container = 'jimmyliu1326/samnsero-r:0.2'
    }
    withName:aggregate_quast {
        container = 'jimmyliu1326/samnsero-r:0.2'
    }
    withName:cctyper {
        container = 'jimmyliu1326/cctyper:1.6.2'
    }
    withName:tnblast {
        container = 'jimmyliu1326/thermonucleotideblast:2.4'
    }
    withName:mafft {
        container = 'quay.io/biocontainers/mafft:7.471--h516909a_0'
    }
    withName:fastp {
        container = 'staphb/fastp:0.23.2'
    }
    withName:shovill {
        container = 'nanozoo/shovill:1.1.0--7e71992'
    }
    withName:fastqc {
        container = 'staphb/fastqc:0.11.9'
    }
    withName:multiqc {
        container = 'quay.io/biocontainers/multiqc:1.13a--pyhdfd78af_1'
    }
    withName:nanocomp {
        container = 'quay.io/biocontainers/nanocomp:1.19.3--pyhdfd78af_0'
    }
}

docker {
    enabled = true
    runOptions = '--rm -u $(id -u):$(id -g)'
}